{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/pages/admin.html"
    },
    {
      "source": "/ladder",
      "destination": "/pages/ladder.html"
    },
    {
      "source": "/matches",
      "destination": "/pages/matches.html"
    },
    {
      "source": "/login",
      "destination": "/pages/login.html"
    },
    {
      "source": "/rules",
      "destination": "/pages/rules.html"
    }
  ]
}
