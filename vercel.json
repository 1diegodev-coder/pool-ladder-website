{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/admin",
      "dest": "/pages/admin.html"
    },
    {
      "src": "/ladder",
      "dest": "/pages/ladder.html"
    },
    {
      "src": "/matches",
      "dest": "/pages/matches.html"
    },
    {
      "src": "/login",
      "dest": "/pages/login.html"
    },
    {
      "src": "/rules",
      "dest": "/pages/rules.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
